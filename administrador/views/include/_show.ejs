<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].titulo %></td>
    <td><%= users[i].ano %></td>
    <td><%= users[i].director %></td>
    <td><%= users[i].actores %></td>
    <td><%= users[i].resena %></td>
    <td id="portada">
        <img width="50px" src="<%= users[i].portada %>"/>
        </td>
    <td><a href="<%= users[i].link %>" target="_blank">Ver Trailer</a></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
        <span class="text-gradient"><i class="fa fa-pencil" aria-hidden="true"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient"><i class="fa fa-times" aria-hidden="true"></i></span>
        </a>
    </td>
</tr>
<% } %>